<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.barmej.notesapp">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UiInterface.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UiInterface.AddNoteActivity" android:parentActivityName=".UiInterface.MainActivity"/>
    <activity android:name=".UiInterface.UpdateNoteDetails" android:parentActivityName=".UiInterface.MainActivity"/>
    <activity android:name=".UiInterface.UpdateNotePhotoDetails" android:parentActivityName=".UiInterface.MainActivity"/>
    <activity android:name=".UiInterface.UpdateNoteCheckDetails" android:parentActivityName=".UiInterface.MainActivity"/>

    </application>

</manifest>